{
  "name": "tw-styleguide-components",
  "version": "2.0.8",
  "description": "TransfeWise Styleguide Components",
  "scripts": {
    "pretest": "bower install",
    "test": "grunt karma",
    "prestart": "bower install",
    "start": "grunt",
    "dev": "live-server --port=8181 --open=demo/index.html",
    "htmllint": "htmllint src/**/*.html; htmllint demo/**/*.html; exit 0",
    "eslint": "eslint src; exit 0",
    "less": "lessc demo/**/*.less demo/css/examples.css",
    "copy-jquery-js": "copyfiles -f node_modules/jquery/dist/jquery.min.js demo/lib/",
    "copy-angular-js": "copyfiles -f node_modules/angular/angular.min.js demo/lib/",
    "copy-bootstrap-js": "copyfiles -f node_modules/bootstrap/dist/js/bootstrap.min.js demo/lib/",
    "copy-bootstrap-css": "copyfiles -f node_modules/bootstrap/dist/css/bootstrap.css demo/lib/",
    "copy-flags-css": "copyfiles -f node_modules/currency-flags/dist/currency-flags.css demo/lib/",
    "copy-demo-js": "npm run copy-jquery-js; npm run copy-angular-js; npm run copy-bootstrap-js;",
    "copy-demo-css": "npm run copy-bootstrap-css; npm run copy-flags-css;",
    "copy-lib": "copyfiles -f dist/js/styleguide-components.js demo/lib/",
    "copy": "npm run copy-demo-css; npm run copy-demo-js;",
    "uglify": "grunt uglify",
    "watch": "grunt watch",
    "npm-start": "npm run htmllint; npm run eslint; npm run copy;",
    "npm-build": "npm run htmllint; npm run eslint; npm run copy-lib;"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/styleguide-components",
    "url": "https://github.com/transferwise/styleguide-components.git"
  },
  "keywords": [
    "AngularJS",
    "Bootstrap"
  ],
  "bugs": {
    "url": "https://github.com/transferwise/styleguide-components/issues"
  },
  "homepage": "https://github.com/transferwise/styleguide-components",
  "dependencies": {
    "angular": "~1.5.8",
    "jquery": "^3.2.1",
    "transferwise-iconfont": "git+ssh://git@github.com/transferwise/iconfont.git"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.11",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "git+https://github.com/transferwise/bootstrap.git",
    "bower": "*",
    "copyfiles": "^1.2.0",
    "currency-flags": "git+https://github.com/transferwise/currency-flags.git",
    "eslint": "^4.6.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "grunt": "^1.0.1",
    "grunt-bower-task": "^0.4.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-less": "^1.2.0",
    "grunt-contrib-uglify": "^1.0.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^20.1.0",
    "grunt-htmllint": "^0.2.7",
    "grunt-karma": "^0.12.1",
    "grunt-webpack": "^3.0.2",
    "grunt-webpack-server": "^0.1.0",
    "html-loader": "^0.5.1",
    "htmllint": "^0.6.0",
    "htmllint-cli": "0.0.6",
    "htmllint-loader": "^1.3.8",
    "iconfont": "git+ssh://git@github.com/transferwise/iconfont.git",
    "jasmine-core": "^2.8.0",
    "jshint-loader": "^0.8.4",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^2.2.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "less": "^2.7.2",
    "live-server": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.5",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
