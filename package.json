{
  "name": "@transferwise/styleguide-components",
  "version": "7.0.1",
  "description": "TransfeWise AngularJS 1.x Styleguide Components",
  "license": "MIT",
  "scripts": {
    "test": "node --icu-data-dir=node_modules/full-icu node_modules/.bin/jest",
    "prestart": "npm install",
    "start": "npm run less && webpack --watch --mode=development;",
    "dev": "live-server --port=1666 --open=demo/index.html",
    "devhttps": "npm run copy:demo && live-server --port=1666 --open=demo/index.html --https=node_modules/live-server-https",
    "build": "npm run less && webpack -p && npm run copy:demo",
    "htmllint": "htmllint src/**/*.html && htmllint src/**/**/*.html && htmllint demo/**/*.html",
    "eslint": "eslint src",
    "lint": "npm run htmllint && npm run eslint",
    "less": "lessc demo/**/*.less demo/css/examples.css",
    "copy:jquery": "cp node_modules/jquery/dist/jquery.min.js demo/lib/",
    "copy:ui-router": "cp node_modules/@uirouter/angularjs/release/angular-ui-router.js demo/lib/",
    "copy:angular": "cp node_modules/angular/angular.min.js demo/lib/",
    "copy:bootstrap-js": "cp node_modules/bootstrap/dist/js/bootstrap.min.js demo/lib/",
    "copy:neptune-styles": "cp node_modules/@transferwise/neptune-css/dist/css/neptune.css demo/lib/; cp node_modules/@transferwise/icons/lib/styles/main.min.css demo/lib/",
    "copy:neptune-fonts": "mkdir demo/fonts; cp node_modules/@transferwise/neptune-css/dist/fonts/*.* demo/fonts",
    "copy:flags": "cp node_modules/currency-flags/dist/currency-flags.css demo/lib/",
    "copy:demo": "mkdir demo/lib; npm run copy:jquery; npm run copy:angular; npm run copy:bootstrap-js; npm run copy:neptune-styles; npm run copy:neptune-fonts; npm run copy:flags; npm run copy:ui-router;",
    "webpack": "webpack --watch",
    "copy-files-to-dist": "mkdir -p dist && cp package.json CHANGELOG.md README.md dist/",
    "deploy": "git-directory-deploy --directory dist --branch releases --username CircleCI --email circleci@transferwise.com",
    "deploy:demo": "gh-pages -d demo",
    "release-to-github": "release-to-github-with-changelog --branch=releases",
    "deploy-for-branch": "deploy-directory-on-branch-to-gh-pages -d demo"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/styleguide-components",
    "url": "https://github.com/transferwise/styleguide-components.git"
  },
  "keywords": [
    "AngularJS",
    "Neptune"
  ],
  "bugs": {
    "url": "https://github.com/transferwise/styleguide-components/issues"
  },
  "homepage": "https://github.com/transferwise/styleguide-components",
  "dependencies": {
    "@transferwise/icons": "^2.3.0",
    "angular": "~1.5.10",
    "commonmark": "^0.29.1",
    "jquery": "^3.4.1",
    "screenfull": "^5.0.2"
  },
  "peerDependencies": {
    "@transferwise/neptune-css": "1.0.1-beta.55"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@types/jest": "^25.1.4",
    "@uirouter/angularjs": "^1.0.22",
    "angular-mocks": "~1.5.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "bootstrap": "git+https://github.com/transferwise/bootstrap.git",
    "canvas": "^2.6.1",
    "css-loader": "^0.28.11",
    "currency-flags": "git+https://github.com/transferwise/currency-flags.git",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "full-icu": "^1.3.1",
    "gh-pages": "^1.1.0",
    "git-directory-deploy": "^1.5.1",
    "html-loader": "^0.5.1",
    "htmllint": "^0.6.0",
    "htmllint-cli": "^0.0.6",
    "intl": "^1.2.5",
    "jest": "^25.2.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "live-server": "^1.1.0",
    "live-server-https": "0.0.2",
    "release-to-github-with-changelog": "^1.2.2",
    "deploy-directory-on-branch-to-gh-pages": "^0.2.8",
    "style-loader": "^0.23.1",
    "@transferwise/neptune-css": "1.0.1-beta.55",
    "uglify-js": "^3.6.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
